{
  "swagger" : "2.0",
  "info" : {
    "version" : "1",
    "title" : "Warenkorb REST Service"
  },
  "basePath" : "/warenkorb-api",
  "schemes" : [ "http" ],
  "paths" : {
    "/warenkorb-api/addToBestellList" : {
      "post" : {
        "summary" : "Fuegt eine Bestellung zur Bestellliste hinzu. ",
        "description" : "",
        "operationId" : "addToBestelltListe",
        "consumes" : [ "application/json" ],
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "in" : "body",
          "name" : "bestellung",
          "description" : "Bestellung",
          "required" : false,
          "schema" : {
            "$ref" : "#/definitions/BestellungDto"
          }
        } ],
        "responses" : {
          "default" : {
            "description" : "successful operation"
          }
        }
      }
    },
    "/warenkorb-api/addToWarenkorb" : {
      "post" : {
        "summary" : "Fuegt ein Produkt zum Warenkorb hinzu. ",
        "description" : "",
        "operationId" : "addToWarenkorb",
        "consumes" : [ "application/json" ],
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "in" : "body",
          "name" : "product",
          "description" : "Produkt",
          "required" : false,
          "schema" : {
            "$ref" : "#/definitions/ProduktDto"
          }
        } ],
        "responses" : {
          "default" : {
            "description" : "successful operation"
          }
        }
      }
    },
    "/warenkorb-api/getAllProducts" : {
      "get" : {
        "summary" : "Gibt alle verfuegbaren Produkte zurück. ",
        "description" : "",
        "operationId" : "getProduktListe",
        "consumes" : [ "application/json" ],
        "produces" : [ "application/json" ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "$ref" : "#/definitions/ProduktListeDto"
            }
          }
        }
      }
    },
    "/warenkorb-api/getWarenkorb" : {
      "get" : {
        "summary" : "Gibt den Warenkorb zurueck. ",
        "description" : "",
        "operationId" : "getWarenkorb",
        "consumes" : [ "application/json" ],
        "produces" : [ "application/json" ],
        "responses" : {
          "200" : {
            "description" : "successful operation",
            "schema" : {
              "$ref" : "#/definitions/WarenkorbDto"
            }
          }
        }
      }
    }
  },
  "securityDefinitions" : {
    "Authorization" : {
      "type" : "apiKey",
      "name" : "Authorization",
      "in" : "header"
    }
  },
