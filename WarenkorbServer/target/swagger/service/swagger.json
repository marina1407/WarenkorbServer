        },
        "vorname" : {
          "type" : "string"
        },
        "nachname" : {
          "type" : "string"
        },
        "adresse" : {
          "type" : "string"
        },
        "ort" : {
          "type" : "string"
        },
        "plz" : {
          "type" : "string"
        },
        "email" : {
          "type" : "string"
        }
      }
    },
    "ProduktDto" : {
      "type" : "object",
      "properties" : {
        "id" : {
          "type" : "integer",
          "format" : "int32"
        },
        "bezeichnung" : {
          "type" : "string"
        },
        "preis" : {
          "type" : "integer",
          "format" : "int32"
        }
      }
    },
    "ProduktListeDto" : {
      "type" : "object",
      "properties" : {
        "produktList" : {
          "type" : "array",
          "items" : {
            "$ref" : "#/definitions/ProduktDto"
